-- Pet Simulator X Mobile Script with Key System (Delta Executor)

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local Humanoid = Character:WaitForChild("Humanoid")
local RootPart = Character:WaitForChild("HumanoidRootPart")

-- ===== KEY SYSTEM =====
local correctKey = "food"
local success, input = pcall(function()
    return LocalPlayer:PromptInput("Enter Key:", "")
end)

if success then
    if input == correctKey then
        print("Key correct! Access granted.")
    else
        print("Wrong key! Access denied")
    end
else
    print("No key input detected, continuing script.")
end

-- ===== SETTINGS =====
local SpeedBoost = 200
local NoclipEnabled = true

-- Apply SpeedBoost
Humanoid.WalkSpeed = SpeedBoost
print("SpeedBoost applied!")

-- Noclip Loop
RunService.Stepped:Connect(function()
    if NoclipEnabled then
        for _, part in pairs(Character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = false
            end
        end
    end
end)
print("Noclip enabled!")

-- Coming Soon message
print("Coming Soon: More features for Pet Simulator X!")
